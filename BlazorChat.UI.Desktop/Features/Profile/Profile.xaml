<reactiveUi:ReactiveUserControl x:TypeArguments="profile:ProfileViewModel" x:Class="BlazorChat.UI.Desktop.Features.Profile.Profile"
                                xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                                xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                                xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                                xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                                xmlns:local="clr-namespace:BlazorChat.UI.Desktop.Features.Profile"
                                xmlns:reactiveUi="http://reactiveui.net"
                                xmlns:profile="clr-namespace:BlazorChat.UI.Shared.Features.Profile;assembly=BlazorChat.UI.Shared"
                                mc:Ignorable="d"
                                d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="3*" />
            <ColumnDefinition Width="5*" />
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="1*"/>
            <RowDefinition Height="9*"/>
        </Grid.RowDefinitions>
        
        <Label Margin="5">Username</Label>
        <TextBox Grid.Row="0" Grid.Column="1" Margin="5" IsReadOnly="True" Name="UsernameTextBox" />
    </Grid>
</reactiveUi:ReactiveUserControl>
